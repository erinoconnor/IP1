<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="233.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IP1New.SearchController">
   <children>
      <Label layoutX="24.0" layoutY="37.0" prefHeight="34.0" prefWidth="299.0" text="Enter the patient's ID you wish to search for:" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="138.0" layoutY="176.0" mnemonicParsing="false" onAction="#displayPatientInf" text="Search">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TextField fx:id="searchID" layoutX="77.0" layoutY="91.0" prefHeight="27.0" prefWidth="185.0" promptText="Patient ID">
         <font>
            <Font size="17.0" />
         </font>
      </TextField>
      <Label visible = "false" fx:id = "cantFindID" layoutX="121.0" layoutY="145.0" text="Can't find patient id" textFill="#d01111" />
   </children>
</AnchorPane>
